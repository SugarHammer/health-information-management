span=\"24\">  \r\n        <el-form-item v-if=\"flag=='yonghu'\" label=\"头像\" prop=\"touxiang\">\r\n          <file-upload\r\n          tip=\"点击上传头像\"\r\n          action=\"file/upload\"\r\n          :limit=\"3\"\r\n          :multiple=\"true\"\r\n          :fileUrls=\"ruleForm.touxiang?ruleForm.touxiang:''\"\r\n          @change=\"yonghutouxiangUploadChange\"\r\n          ></file-upload>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='yonghu'\"  label=\"手机\" prop=\"shouji\">\r\n          <el-input v-model=\"ruleForm.shouji\"               placeholder=\"手机\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='yonghu'\"  label=\"邮箱\" prop=\"youxiang\">\r\n          <el-input v-model=\"ruleForm.youxiang\"               placeholder=\"邮箱\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                                                <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='xinlizixunshi'\"  label=\"咨询师账号\" prop=\"zixunshizhanghao\">\r\n          <el-input v-model=\"ruleForm.zixunshizhanghao\" readonly              placeholder=\"咨询师账号\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='xinlizixunshi'\"  label=\"密码\" prop=\"mima\">\r\n          <el-input v-model=\"ruleForm.mima\"               placeholder=\"密码\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='xinlizixunshi'\"  label=\"咨询师姓名\" prop=\"zixunshixingming\">\r\n          <el-input v-model=\"ruleForm.zixunshixingming\"               placeholder=\"咨询师姓名\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='xinlizixunshi'\"  label=\"性别\" prop=\"xingbie\">\r\n          <el-select v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\">\r\n            <el-option\r\n                v-for=\"(item,index) in xinlizixunshixingbieOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='xinlizixunshi'\"  label=\"年龄\" prop=\"nianling\">\r\n          <el-input v-model=\"ruleForm.nianling\"               placeholder=\"年龄\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"24\">  \r\n        <el-form-item v-if=\"flag=='xinlizixunshi'\" label=\"资格证书\" prop=\"zigezhengshu\">\r\n          <file-upload\r\n          tip=\"点击上传资格证书\"\r\n          action=\"file/upload\"\r\n          :limit=\"3\"\r\n          :multiple=\"true\"\r\n          :fileUrls=\"ruleForm.zigezhengshu?ruleForm.zigezhengshu:''\"\r\n          @change=\"xinlizixunshizigezhengshuUploadChange\"\r\n          ></file-upload>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='xinlizixunshi'\"  label=\"地址\" prop=\"dizhi\">\r\n          <el-input v-model=\"ruleForm.dizhi\"               placeholder=\"地址\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"12\">\r\n        <el-form-item   v-if=\"flag=='xinlizixunshi'\"  label=\"手机号\" prop=\"shoujihao\">\r\n          <el-input v-model=\"ruleForm.shoujihao\"               placeholder=\"手机号\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n                        <el-col :span=\"24\">  \r\n        <el-form-item v-if=\"flag=='xinlizixunshi'\" label=\"照片\" prop=\"zhaopian\">\r\n          <file-upload\r\n          tip=\"点击上传照片\"\r\n          action=\"file/upload\"\r\n          :limit=\"3\"\r\n          :multiple=\"true\"\r\n          :fileUrls=\"ruleForm.zhaopian?ruleForm.zhaopian:''\"\r\n          @change=\"xinlizixunshizhaopianUploadChange\"\r\n          ></file-upload>\r\n        </el-form-item>\r\n      </el-col>\r\n                                                                                                                                                                        <el-form-item v-if=\"flag=='users'\" label=\"用户名\" prop=\"username\">\r\n        <el-input v-model=\"ruleForm.username\" \r\n        placeholder=\"用户名\"></el-input>\r\n      </el-form-item>\r\n      <el-col :span=\"24\">\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onUpdateHandler\">修 改</el-button>\r\n      </el-form-item>\r\n      </el-col>\r\n      </el-row>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n<script>\r\n// 数字，邮件，手机，url，身份证校验\r\nimport { isNumber,isIntNumer,isEmail,isMobile,isPhone,isURL,checkIdCard } from \"@/utils/validate\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      ruleForm: {},\r\n      flag: '',\r\n      usersFlag: false,\r\n                                                                              yonghuxingbieOptions: [],\r\n                                                                                                                        xinlizixunshixingbieOptions: [],\r\n                                                                                                                                                                                                                };\r\n  },\r\n  mounted() {\r\n    var table = this.$storage.get(\"sessionTable\");\r\n    this.flag = table;\r\n    this.$http({\r\n      url: `${this.$storage.get(\"sessionTable\")}/session`,\r\n      method: \"get\"\r\n    }).then(({ data }) => {\r\n      if (data && data.code === 0) {\r\n        this.ruleForm = data.data;\r\n      } else {\r\n        this.$message.error(data.msg);\r\n      }\r\n    });\r\n                                                    this.yonghuxingbieOptions = \"男,女\".split(',')\r\n                                                                                this.xinlizixunshixingbieOptions = \"男,女\".split(',')\r\n                                                                                                                                          },\r\n  methods: {\r\n                                                                                                                                                                                                                                                                                                                        yonghutouxiangUploadChange(fileUrls) {\r\n        this.ruleForm.touxiang = fileUrls;\r\n    },\r\n                                                                                                                xinlizixunshizhaopianUploadChange(fileUrls) {\r\n        this.ruleForm.zhaopian = fileUrls;\r\n    },\r\n                                                                                                    onUpdateHandler() {\r\n                                          if((!this.ruleForm.yonghuming)&& 'yonghu'==this.flag){\r\n        this.$message.error('用户名不能为空');\r\n        return\r\n      }\r\n                                                                  if((!this.ruleForm.mima)&& 'yonghu'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n                                                                  if((!this.ruleForm.yonghuxingming)&& 'yonghu'==this.flag){\r\n        this.$message.error('用户姓名不能为空');\r\n        return\r\n      }\r\n                                                                                                                                                                                                      if( 'yonghu' ==this.flag && this.ruleForm.shouji&&(!isMobile(this.ruleForm.shouji))){\r\n        this.$message.error(`手机应输入手机格式`);\r\n        return\r\n      }\r\n                                                                        if( 'yonghu' ==this.flag && this.ruleForm.youxiang&&(!isEmail(this.ruleForm.youxiang))){\r\n        this.$message.error(`邮箱应输入邮箱格式`);\r\n        return\r\n      }\r\n                                                            if((!this.ruleForm.zixunshizhanghao)&& 'xinlizixunshi'==this.flag){\r\n        this.$message.error('咨询师账号不能为空');\r\n        return\r\n      }\r\n                                                                  if((!this.ruleForm.mima)&& 'xinlizixunshi'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n                                                                  if((!this.ruleForm.zixunshixingming)&& 'xinlizixunshi'==this.flag){\r\n        this.$message.error('咨询师姓名不能为空');\r\n        return\r\n      }\r\n                                                                                                                              if( 'xinlizixunshi' ==this.flag && this.ruleForm.nianling&&(!isIntNumer(this.ruleForm.nianling))){\r\n       this.$message.error(`年龄应输入整数`);\r\n        return\r\n      }\r\n                                                                                                                                                                                                if( 'xinlizixunshi' ==this.flag && this.ruleForm.shoujihao&&(!isMobile(this.ruleForm.shoujihao))){\r\n        this.$message.error(`手机号应输入手机格式`);\r\n        return\r\n      }\r\n                                                                                                                                                                                                                                                                                                                                                            this.$http({\r\n        url: `${this.$storage.get(\"sessionTable\")}/update`,\r\n        method: \"post\",\r\n        data: this.ruleForm\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.$message({\r\n            message: \"修改信息成功\",\r\n            type: \"success\",\r\n            duration: 1500,\r\n            onClose: () => {\r\n            }\r\n          });\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"],"sourceRoot":"src/views"}]}